function  op1 ( ) {

    var  n1  =  10 ;
    var  n2  =  5 ;
    var  resul ;

    si ( resultado  >  10 ) {
        consola . log ( "el resultado es mayor que 10" )
    } más {
        consola . log ( "el resultado es menor que 10" )
    }

}

function  op2 ( ) {
    var  n3  =  10 ;

    para ( i  =  0 ;  i < n3  ;  i ++  ) {
        consola . log ( i ) ;
    }

}
function  op3 ( ) {
    cont  =  0 ;

    while ( cont  <  5 ) {
        cont ++  ; 
        consola . log ( cont ) ;
    }
}
